variance <- rep(0,10)
updateVariance <- function(){
for(j in 1:nrow(mtrx)){
temp <- 0
for(i in 2:ncol(mtrx)){
temp <- temp + abs(mtrx[i,j]-mtrx[1,j])
}
variance[j] <- temp/(nrow(mtrx)-1)
}
}
# Main
updateVariance()
